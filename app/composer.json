{
    "name": "karlito-web/asphalt",
    "homepage": "https://github.com/Karlito15/asphalt",
    "description": "Asphalt 9",
    "keywords": ["php", "apache", "symfony", "composer", "bootstrap", "font-awesome"],
    "authors": [
        {
            "name": "Karlito15",
            "email": "giancarlo.palumbo@free.fr",
            "homepage": "https://www.giancarlo-palumbo.myds.me/",
            "role": "Freelance"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "^1.11.99.5",
        "doctrine/dbal": "^3.10.3",
        "doctrine/doctrine-bundle": "^2.18.0",
        "doctrine/doctrine-migrations-bundle": "^3.5.0",
        "doctrine/orm": "^3.5.2",
        "easycorp/easyadmin-bundle": "^4.26.5",
        "league/csv": "^9.27.0",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/apache-pack": "v1.0.1",
        "symfony/asset": "7.3.*",
        "symfony/asset-mapper": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-messenger": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/event-dispatcher": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2.8.2",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/mailer": "7.3.*",
        "symfony/mime": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.3.*",
        "symfony/process": "7.3.*",
        "symfony/property-access": "7.3.*",
        "symfony/property-info": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/serializer": "7.3.*",
        "symfony/stimulus-bundle": "^2.30",
        "symfony/string": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-twig-component": "^2.30",
        "symfony/validator": "7.3.*",
        "symfony/web-link": "7.3.*",
        "symfony/yaml": "7.3.*",
        "symfonycasts/sass-bundle": "^0.8.3",
        "twbs/bootstrap": "^5.3",
        "twig/cache-extra": "^3.21",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/intl-extra": "^3.21",
        "twig/string-extra": "^3.21",
        "twig/twig": "^2.12|^3.21.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.2",
        "fakerphp/faker": "^1.24.1",
        "phpstan/phpstan-symfony": "^2.0.8",
        "phpunit/phpunit": "^11.5.42",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/debug-bundle": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3.4",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "vendor-dir": "vendor/"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "sass:build": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*",
            "docker": true
        }
    }
}
