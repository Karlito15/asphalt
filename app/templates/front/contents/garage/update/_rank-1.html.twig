<section class="{{ container|default('container') }} mb-2">
    <div class="card mx-5">
        <div class="card-header bg-secondary py-1">
            <h2 class="h6 text-center m-0">{{ 'text.rank'|trans|plural }}</h2>
        </div>
        <div class="card-body pt-0">
            <div class="card-group">
                <twig:FormGarageSimple form="{{ form.rank.0.star0 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star1 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star2 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star3 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star4 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star5 }}" />
                <twig:FormGarageSimple form="{{ form.rank.0.star6 }}" />
            </div>
        </div>
    </div>
</section>
