<form method="post" action="{{ path('app.garage.delete', {'id': garage.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ garage.id) }}">
    <button class="btn btn-outline-danger text-uppercase w-100">Delete</button>
</form>
