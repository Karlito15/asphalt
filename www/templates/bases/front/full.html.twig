{% extends '@App/bases/index.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <!-- Stylesheet -->
    <link href="{{ asset('themes/aero/assets/plugins/bootstrap/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('themes/aero/assets/plugins/jvectormap/jquery-jvectormap-2.0.3.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('themes/aero/assets/plugins/charts-c3/plugin.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('themes/aero/assets/plugins/morrisjs/morris.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- Custom Css -->
    <link href="{{ asset('themes/aero/assets/css/style.min.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}

{% block body %}<body class="theme-blush">{% endblock %}

{% block containers %}
    {% block preloader %}
        {% include '@App/partials/front/loader.html.twig' %}
    {% endblock %}

    {% block search %}
        {% include '@App/partials/front/search.html.twig' %}
    {% endblock %}

    {% block navbar %}
        {% include '@App/partials/front/navbar.html.twig' %}
    {% endblock %}

    {% block leftsidebar %}
        {% include '@App/partials/front/sidebar-left.html.twig' %}
    {% endblock %}

    {% block rightsidebar %}
        {% include '@App/partials/front/sidebar-right.html.twig' %}
    {% endblock %}

    {% block content %}
        {% include('@App/contents/front/index.html.twig') %}
    {% endblock %}

{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <!-- Jquery Core Js -->
    <script src="{{ asset('themes/aero/assets/bundles/libscripts.bundle.js') }}" type="text/javascript"></script>
    <!-- Lib Scripts Plugin Js ( jquery.v3.2.1, Bootstrap4 js) -->
    <script src="{{ asset('themes/aero/assets/bundles/vendorscripts.bundle.js') }}" type="text/javascript"></script>
    <!-- slimscroll, waves Scripts Plugin Js -->
    <script src="{{ asset('themes/aero/assets/bundles/jvectormap.bundle.js') }}" type="text/javascript"></script>
    <!-- JVectorMap Plugin Js -->
    <script src="{{ asset('themes/aero/assets/bundles/sparkline.bundle.js') }}" type="text/javascript"></script>
    <!-- Sparkline Plugin Js -->
    <script src="{{ asset('themes/aero/assets/bundles/c3.bundle.js') }}" type="text/javascript"></script>
    <script src="{{ asset('themes/aero/assets/bundles/mainscripts.bundle.js') }}" type="text/javascript"></script>
    <script src="{{ asset('themes/aero/assets/js/pages/index.js') }}" type="text/javascript"></script>
{% endblock %}
