{% extends '@App/bases/front/full.html.twig' %}

{% block title %}{{ controller_name }}{% endblock %}

{% block content %}
    <section class="content">
        <div class="row">
            <div class="col-8">
                <h2 class="h4 text-center">{{ 'app.dashboard.index.coins'|trans }}</h2>
                {% include '@App/contents/front/dashboard/_moneys.html.twig' %}
                <h2 class="h4 text-center">{{ 'app.dashboard.index.jokers'|trans }}</h2>
                {% include '@App/contents/front/dashboard/_joker-S.html.twig' %}
                {% include '@App/contents/front/dashboard/_joker-A.html.twig' %}
