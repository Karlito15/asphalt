                {% include '@App/contents/front/dashboard/_joker-C.html.twig' %}
                {% include '@App/contents/front/dashboard/_joker-D.html.twig' %}
                <h2 class="h4 text-center">{{ 'app.dashboard.index.imports'|trans }}</h2>
                {% include '@App/contents/front/dashboard/_import-rares.html.twig' %}
                <h2 class="h4 text-center">{{ 'app.dashboard.index.commons'|trans }}</h2>
                {% include '@App/contents/front/dashboard/_import-commons.html.twig' %}
            </div>
            <div class="col-4">
                <h2 class="h4 text-center">{{ 'app.dashboard.index.stats'|trans }}</h2>
