

<div class="card-body">
    <div class="card-title"><h4 class="text-dark text-center h6">{{ 'app.garage.rank'|trans }}</h4></div>
    <table class="table">
        <thead>
            <tr>
                <th class="text-center py-1">{{ 'table.start'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star1'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star2'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star3'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star4'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star5'|trans }}</th>
                <th class="text-center py-1">{{ 'table.star6'|trans }}</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                   {{ component('FormGarageRead', {name: 'table.start', value: entity.rank_star0 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star1', value: entity.rank_star1 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star2', value: entity.rank_star2 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star3', value: entity.rank_star3 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star4', value: entity.rank_star4 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star5', value: entity.rank_star5 }) }}
                </td>
                <td>
                   {{ component('FormGarageRead', {name: 'table.star6', value: entity.rank_star6 }) }}
                </td>
            </tr>
        </tbody>
    </table>
</div>
