<form method="post" action="{{ path('app_mission_app_delete', {'id': mission_app.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ mission_app.id) }}">
    <button class="btn btn-outline-danger">{{ 'btn.delete' | trans }}</button>
</form>
